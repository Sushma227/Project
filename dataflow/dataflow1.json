{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_DLsqlserver",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "derivedColumn4"
				},
				{
					"name": "lookup1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Invoice as string,",
				"          StockCode as string,",
				"          Description as string,",
				"          Quantity as short,",
				"          InvoiceDate as string,",
				"          Price as double,",
				"          Customer_ID as short,",
				"          Country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"derivedColumn2 derive() ~> derivedColumn1",
				"derivedColumn3 derive() ~> derivedColumn2",
				"derivedColumn4 derive() ~> derivedColumn3",
				"lookup1 derive() ~> derivedColumn4",
				"source1 lookup(,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1"
			]
		}
	}
}