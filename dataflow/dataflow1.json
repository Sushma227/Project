{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_DLsqlserver",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Invoice as string 'Int',",
				"          StockCode as string 'Int',",
				"          Description as string 'nvarchar',",
				"          Quantity as short 'short',",
				"          InvoiceDate as string 'date',",
				"          Price as double 'double',",
				"          Customer_ID as short 'int',",
				"          Country as string 'string'",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     fileList: true) ~> source1"
			]
		}
	}
}